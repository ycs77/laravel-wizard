{
    "name": "ycs77/laravel-wizard",
    "description": "A web setup wizard for Laravel application.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "wizard",
        "step",
        "steps",
        "component",
        "plugin"
    ],
    "homepage": "https://github.com/ycs77/laravel-wizard",
    "require": {
        "php": "^7.1.3",
        "facade/ignition-contracts": "^1.0",
        "illuminate/contracts": "~5.6|~5.7|~5.8|^6.0|^7.0|^8.0",
        "laravel/framework": "~5.6|~5.7|~5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "~3.6|~3.7|~3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ycs77\\LaravelWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ycs77\\LaravelWizard\\Test\\": "tests"
        },
        "classmap": [
            "tests/Stubs/database/migrations"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ycs77\\LaravelWizard\\WizardServiceProvider"
            ],
            "aliases": {
                "Wizard": "Ycs77\\LaravelWizard\\Facades\\Wizard"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
